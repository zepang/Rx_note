>(</span><span class=3D"token string">"link"</span><span class=3D"token pun=
ctuation">)</span><span class=3D"token punctuation">;</span>
link<span class=3D"token punctuation">.</span>href <span class=3D"token ope=
rator">=3D</span> <span class=3D"token string">"myscript.js"</span><span cl=
ass=3D"token punctuation">;</span>
